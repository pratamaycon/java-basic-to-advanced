package application;

import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.util.Map;
import java.util.TreeMap;

public class ExerProgramMap {

	public static void main(String[] args) {
	
		String path = "C:\\Users\\Maycon.Conceicao\\Documents\\projetos\\proway\\nelioAlves\\src\\input.txt";
		
		Map<String, Integer> canditados = new TreeMap<>();
		
		try (BufferedReader br = new BufferedReader(new FileReader(path))) {
			
			
			
			String line = br.readLine();
			while (line != null) {

				String[] fields = line.split(",");
				
				int votos = 0;
				System.out.println(fields[0] + "       dasdasd");
				if (fields[0] == "Alex Blue") {
					System.out.println(fields[0]);
					votos = Integer.parseInt(fields[1]);
				}
				
				canditados.put(fields[0], votos);

				line = br.readLine();
			}
			
			System.out.println(canditados);
			
			
		} catch (IOException e) {
			System.out.println("Error: " + e.getMessage());
		}

	}

}
